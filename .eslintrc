{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "html"
    ],
    "settings": {
      "html/xml-extensions": [".html",".vue"]
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", 140],
        "no-shadow":0,
        "arrow-parens":0,
        "no-param-reassign":0,
        "no-console":0,
        "func-names":0
    }
}